<%- layout('style') -%>
<html>
<head>
    <meta charset="utf-8">
    <title>layui</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="./css/font.css">
    <link rel="stylesheet" href="./css/xadmin.css">
    <link rel="stylesheet" href="./lib/layui/css/layui.css"  media="all">
    <script type="text/javascript" src="./js/xadmin.js"></script>
  <!-- 注意：如果你直接复制所有代码到本地，上述css路径需要改成你本地的 -->
</head>
<body>
    <!--<div class="x-nav">
        <span class="layui-breadcrumb">
          <a href="">首页</a>
          <a href="">演示</a>
          <a>
            <cite>导航元素</cite></a>
        </span>
      </div>-->

        <div class="x-body">
            <div class="demoTable">
                <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
                    <legend>资产负债表 (2018年5月30日,人民币)</legend>
                </fieldset>
              <!--<blockquote class="layui-elem-quote layui-quote-nm">
                  2018年5月30日
                  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                  货币单位：人民币元
              </blockquote>-->
              </div>
            

                <table class="layui-table" lay-data="{url:'/api/blsheet', height: 'full-200', cellMinWidth: 80, page: true,
                limit:100, initSort: {field:'assnum', type:'asc'}}">
                  <thead>
                    <tr>
                      <th lay-data="{field:'assproj', width:180}">资产</th>
                      <th lay-data="{field:'assnum', width:60, align:'center', sort: true}">行次</th>
                      <th lay-data="{field:'assendbln', width:120, align:'center'}">期末余额</th>
                      <th lay-data="{field:'assstartbln', width:120, align:'center'}">年初余额</th>
                
                      <th lay-data="{field:'liabproj', width:200}">负债及所有者权益</th>
                      <th lay-data="{field:'liabnum', width:60, align:'center', sort: true}">行次</th>
                      <th lay-data="{field:'liabendbln', width:120, align:'center'}">期末余额</th>
                      <th lay-data="{field:'liabstartbln', width:120, align:'center'}">年初余额</th>
                    </tr>
                  </thead>
                </table>
            </div>


               
          
<script type="text/javascript" src="./lib/layui/layui.js" charset="utf-8"></script>
<!-- 注意：如果你直接复制所有代码到本地，上述js路径需要改成你本地的 -->
<script>
layui.use('table', function(){
  var table = layui.table;
  
});
</script>

</body>
</html>